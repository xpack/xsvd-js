{
  "name": "xsvd",
  "version": "0.1.3",
  "description": "The xPack SVD manager",
  "main": "./lib/main.js",
  "directories": {
    "bin": "./bin",
    "lib": "./lib",
    "test": "./test"
  },
  "bin": {
    "xsvd": "./bin/xsvd.js"
  },
  "scripts": {
    "test": "standard && npm run test-tap",
    "test-coverage": "tap --coverage --reporter=classic --timeout 600 \"test/tap/*.js\"",
    "test-tap": "tap --reporter=classic --timeout 300 \"test/tap/*.js\"",
    "tap": "tap --reporter=spec --timeout 300",
    "fix": "standard --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xpack/xsvd-js.git"
  },
  "keywords": [
    "xpack",
    "svd",
    "cmsis"
  ],
  "author": {
    "name": "Liviu Ionescu",
    "email": "ilg@livius.net",
    "url": "http://liviusdotnet.wordpress.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/xpack/xsvd-js/issues"
  },
  "homepage": "https://github.com/xpack/xsvd-js",
  "dependencies": {
    "npmlog": "^4.0.2",
    "wscript-avoider": "~3.0.0"
  },
  "devDependencies": {
    "should": "^11.2.1",
    "standard": "*",
    "tap": "*"
  },
  "peerDependencies": {},
  "bundledDependencies": [],
  "optionalDependencies": {},
  "engines": {
    "node": ">7.7.0"
  }
}
